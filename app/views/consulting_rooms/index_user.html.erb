<div class="d-sm-flex align-items-center justify-content-between mb-3">
  <p class="h2 m-0 text-gray-800">Consultorios</p>
</div>
<div class="d-flex justify-content-between">
  <div class="col-lg-6 col-md-6 col-sm-12">
    <div class="row">
      <%# Semillas temporales %>
      <div>
        <% forma_pago = ['efectivo', 'punto de venta', 'zelle', 'paypal'] %>
        <% if user_signed_in? %>
          <% if !current_user.is_vet %>
            <div class="searcher_user">
              <%= form_tag consulting_rooms_user_path, class: "form-size-search", method: :get do %>
                <%= text_field_tag :query,
                      params[:query],
                      class: "form-control",
                      id: 'buscar',
                      placeholder: "Buscar veterinarios"
                    %>
                <%= submit_tag "", class: "fas fa-search size-button-search", id: "search-f" %>
              <% end %>
            </div>
          <% end %>
        <% else %>
          <div class="searcher_user">
            <%= form_tag consulting_rooms_user_path, method: :get do %>
              <%= text_field_tag :query,
                    params[:query],
                    class: "form-control",
                    id: 'buscar',
                    placeholder: "Buscar veterinarios"
                  %>
              <%= submit_tag "", class: "fas fa-search size-button-search", id: "search-f" %>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
    <div id="global_user">
            <%# Lista de consultorios %>
        <%= render 'shared/consulting_list' %>
    </div>
  </div>
  <div id="map" class="col-md-6 col-sm-12 map-custom_user"
      data-markers="<%= @markers.to_json %>"
      data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>
</div>
