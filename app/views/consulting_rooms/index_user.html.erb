<div class="row">
  <div class="col-md-6 col-sm-12">
    <div class="row">
      <p class="h2 m-0 text-gray-600 w-100 text-center">Consultorios Veterinarios</p>
    </div>
    <div class="d-flex justify-content-center">
      <div>
        <%= form_tag consulting_rooms_user_path, method: :get do %>
          <%= text_field_tag :query,
                      params[:query],
                      class: "form-control",
                      id: 'buscar',
                      placeholder: "Buscar veterinarios"
                    %>
          <%= submit_tag "ï€‚", class: "fas fa-search search-f-user" %>
        <% end %>
      </div>
    </div>
    <div id="global" class="row ">
      <%# Lista de consultorios %>
      <%= render 'shared/consulting_list' %>
    </div>
  </div>
  <div class="col-md-6 col-sm-12">
    <div id="map" class="map-custom_user" data-markers="<%= @markers.to_json %>"
      data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
    </div>
  </div>
</div>
